command script import ~/.lldb/fsa.py

command regex pod 's/^(.+)$/expression -O -- [[NSString alloc] initWithData:%1 encoding:4]/' -h 'Print NSData converted to NSString.'
command regex poj 's/^(.+)$/expression -O -- [NSJSONSerialization JSONObjectWithData:%1 options:0 error:nil]/' -h 'Print NSData converted to a JSON object.'
